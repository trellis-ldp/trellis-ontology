@prefix asfext: <http://projects.apache.org/ns/asfext#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

<https://www.trellisldp.org/> a doap:Project ;
    doap:license <http://www.apache.org/licenses/LICENSE-2.0> ;
    doap:homepage <https://www.trellisldp.org/> ;
    doap:shortdesc "Trellis is a linked data server" ;
    doap:description "Trellis is a modular linked data server that can be used for managing and disseminating digital content. It implements the Linked Data Platform" ;
    doap:wiki <https://github.com/trellis-ldp/trellis/wiki> ;
    doap:programming-language "Java" ;
    doap:repository
        [ a doap:GitRepository ;
            doap:location <https://github.com/trellis-ldp/trellis.git> ;
            doap:browse <https://github.com/trellis-ldp/trellis> ] ,
        [ a doap:GitRepository ;
            doap:location <https://github.com/trellis-ldp/trellis-rosid.git> ;
            doap:browse <https://github.com/trellis-ldp/trellis-rosid> ] ;
    doap:maintainer <https://acoburn.people.amherst.edu/#i> ;
    doap:developer <https://acoburn.people.amherst.edu/#i> ;
    asfext:implements
        [ a asfext:Standard ;
            asfext:title "Linked Data Platform" ;
            asfext:body "The W3C Linked Data Platform" ;
            asfext:id "ldp" ;
            asfext:url <https://www.w3.org/TR/ldp/> ] ,
        [ a asfext:Standard ;
            asfext:title "Activity Streams" ;
            asfext:body "W3C Activity Streams 2.0" ;
            asfext:id "as" ;
            asfext:url <https://www.w3.org/TR/activitystreams-core/> ] ,
        [ a asfext:Standard ;
            asfext:title "Memento" ;
            asfext:body "RFC 7089: HTTP Framework for Time-Based Access to Resource States" ;
            asfext:id "memento" ;
            asfext:url <https://tools.ietf.org/html/rfc7089> ] ,
        [ a asfext:Standard ;
            asfext:title "Instance Digests" ;
            asfext:body "RFC 3230: Instance Digests in HTTP" ;
            asfext:id "digest" ;
            asfext:url <https://tools.ietf.org/html/rfc3230> ] ,
        [ a asfext:Standard ;
            asfext:title "Web Access Control" ;
            asfext:body "SOLID WebAC: Authorization and Access Control" ;
            asfext:id "webac" ;
            asfext:url <https://github.com/solid/solid-spec#authorization-and-access-control> ] .
